#. Plugin Name of the plugin
msgid "Plugin Update Disabler"
msgstr "Блокировщик обновлений плагинов"

#. Description of the plugin
msgid "Allows to forcefully block updates for selected plugins and disable all update notifications"
msgstr "Позволяет принудительно блокировать обновления для выбранных плагинов и отключать все уведомления об обновлениях"

#: plugin-update-disabler.php:96
msgid "Block updates"
msgstr "Блокировать обновления"

#: plugin-update-disabler.php:97
msgid "Unblock updates"
msgstr "Разблокировать обновления"

#: plugin-update-disabler.php:143
msgid "Plugin not specified"
msgstr "Плагин не указан"

#: plugin-update-disabler.php:176
msgid "Updating this plugin may harm the site. Continue?"
msgstr "Обновление этого плагина может повредить сайт. Продолжить?"

#: plugin-update-disabler.php:177
msgid "Blocking updates..."
msgstr "Блокировка обновлений..."

#: plugin-update-disabler.php:178
msgid "Unblocking updates..."
msgstr "Разблокировка обновлений..."

#: plugin-update-disabler.php:179
msgid "Update block status changed."
msgstr "Статус блокировки обновлений изменен."

#: plugin-update-disabler.php:181
msgid "Invalid action"
msgstr "Недопустимое действие"